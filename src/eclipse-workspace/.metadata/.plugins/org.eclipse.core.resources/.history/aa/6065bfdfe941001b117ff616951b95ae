/*----------------------------------------------------------------------------------------------------------------------
	 Alttire karakteri sabit önündeki eklerden öncei sonra veya arada kullanılamaz
---------------------------------------------------------------------------------------------------------------------*/
package csd;

class App {
	public static void main(String [] args) 
	{		
		double a = .2; //0.2
		double b = 3.; //3.0
		
		System.out.printf("a=%f%n", a);				
		System.out.printf("b=%f%n", b);
	}
}

